<%-include('partials/header')%>

<h1 style="color: rgb(213, 80, 80)">Compose</h1>

<form class="compose" action="/compose" method="post">
  <div class="mb-3">
    <label for="exampleFormControlInput1" class="form-label">Title</label>
    <input
      name="postTitle"
      type="text"
      class="form-control"
      id="exampleFormControlInput1"
      placeholder="Enter your text.."
      fdprocessedid="p8zzm"
      autocomplete="off"
    />

    <label>Post</label>
    <br/>
    <textarea
      name="postBody"
      class="form-control"
      id="exampleFormControlTextarea1"
      rows="7"
      cols="152"
      placeholder="Enter your text here.."
      autocomplete="off"
    ></textarea>
  </div>

  <br/>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>

<%- include('partials/footer') %>
